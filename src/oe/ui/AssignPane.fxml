<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="oe.ui.AssignDialogPaneController">
   <children>
      <GridPane prefHeight="400.0" prefWidth="601.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="296.0" minWidth="10.0" prefWidth="239.0" />
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="398.0" minWidth="10.0" prefWidth="362.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Label text="Assign to" GridPane.halignment="RIGHT" GridPane.rowIndex="1">
               <padding>
                  <Insets right="50.0" />
               </padding>
            </Label>
            <ComboBox fx:id="assignComboBox" prefWidth="150.0" GridPane.columnIndex="1" GridPane.rowIndex="1">
               <contextMenu>
               </contextMenu>
            </ComboBox>
            <Button fx:id="ApplyButton" mnemonicParsing="false" text="Apply" GridPane.halignment="RIGHT" GridPane.rowIndex="2" GridPane.valignment="CENTER" onAction="#handleApplyButtonAction"/>
            <Button fx:id="CancelButton" mnemonicParsing="false" text="Cancel" GridPane.columnIndex="1" GridPane.halignment="CENTER" GridPane.rowIndex="2" onAction="#handleCancelButtonAction"/>
            <Label fx:id="CheckedActionItem" text="" GridPane.halignment="CENTER" GridPane.rowIndex="1">
               <GridPane.margin>
                  <Insets right="10.0" />
               </GridPane.margin></Label>
         </children>
      </GridPane>
   </children>
</AnchorPane>
